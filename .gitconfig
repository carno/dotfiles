[push]
	default = current
[alias]
	co = checkout
	ci = commit
	up = pull
	st = status -sb
	lg = log --graph --pretty=format:'%C(cyan)%h%C(reset) :%C(yellow)%d%C(reset) %s %C(green)(%cr) %C(red)[%an]%C(reset)' --abbrev-commit --date=relative --date-order
	ll = log --pretty=format:'%C(cyan)%h%C(reset) :%C(yellow)%d %C(reset)%s%C(red) [%cn]' --decorate --numstat
	ups = submodule foreach git pull origin master
	dt = difftool
	mt = mergetool
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
	log = true
[difftool]
	prompt = false
[mergetool]
	prompt = false
	keepBackup = false
[color]
	ui = auto
[include]
	path = ~/.gitlocal
[branch "master"]
	rebase = true
